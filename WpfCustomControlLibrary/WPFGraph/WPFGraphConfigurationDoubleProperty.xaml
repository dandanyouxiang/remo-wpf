<UserControl x:Class="DNBSoft.WPF.WPFGraph.WPFGraphConfigurationDoubleProperty"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="*" />
            <ColumnDefinition Width="100" />
        </Grid.ColumnDefinitions>
        <Label Name="nameLabel" VerticalAlignment="Center" Grid.Column="0">
            Property Name
        </Label>
        <TextBox Name="valueTextBox" VerticalAlignment="Center" Grid.Column="1" TextChanged="valueTextBox_TextChanged" />
    </Grid>
</UserControl>
